{
  "editor.tabSize": 2,
  "editor.rulers": [
    80,
    120
  ],
  "typescript.autoClosingTags": false,
  "typescript.format.semicolons": "insert",
  "typescript.preferences.quoteStyle": "single",
  "eslint.run": "onSave",
  "eslint.options": {
  "eslint.validate": [
    {
      "language": "typescript",
      "autoFix": true
    }
  ],
  "eslint.autoFixOnSave": true
  },
  "auto-close-tag.disableOnLanguage": [
    "typescript"
  ]
}